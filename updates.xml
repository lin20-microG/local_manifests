  <project path="frameworks/native" name="lin20-microG/android_frameworks_native" groups="pdk" />
  <remove-project name="LineageOS/android_libcore" />
  <project path="libcore" name="lin20-microG/android_libcore" groups="pdk" />
  <remove-project name="LineageOS/android_packages_apps_Jelly" />
